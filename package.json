    }
  },
  "lint-staged": {
    "src/**/*.js": "eslint --fix ",
    "src/**/*.jsx": "eslint"
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ],
    "rules": {
      "subject-case": [
        0
      ]
    }
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ]
