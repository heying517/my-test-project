    "html-webpack-harddisk-plugin": "^1.0.1",
    "husky": "^1.1.4",
    "imagemin-webpack-plugin": "^2.4.0",
    "lint-staged": "^8.0.4",
    "lodash": "^4.17.11",
    "react-app-rewire-less": "^2.1.3",
    "react-app-rewire-webpack-bundle-analyzer": "^1.0.1",
    "react-app-rewired": "^1.6.2",
    "react-scripts": "2.1.1",
    "script-ext-html-webpack-plugin": "^2.1.3",
